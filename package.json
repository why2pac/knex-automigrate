{
  "name": "knex-automigrate",
  "version": "0.1.2",
  "description": "Table schema based database migration tool, built on top of the knex.js",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --detectOpenHandles",
    "test:migrate": "node bin/cli.js migrate:auto --safe --knexfile ./test/migration/knex.config.js --cwd ./test/migration",
    "check": "npm-check",
    "lint": "eslint ./",
    "lintfix": "eslint ./ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/why2pac/knex-automigrate.git"
  },
  "keywords": [
    "nodejs",
    "knexjs",
    "database",
    "orm",
    "table",
    "migration"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "liftoff": "^3.1.0",
    "minimist": "^1.2.6",
    "tildify": "^2.0.0"
  },
  "peerDependencies": {
    "knex": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.7.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.11.4",
    "ts-jest": "^29.2.5"
  },
  "bin": {
    "knex-automigrate": "./bin/cli.js"
  },
  "files": [
    "README.md",
    "bin/*",
    "lib/*",
    "index.js",
    "LICENSE"
  ],
  "author": "GONZO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/why2pac/knex-automigrate/issues"
  },
  "homepage": "https://github.com/why2pac/knex-automigrate#readme"
}
